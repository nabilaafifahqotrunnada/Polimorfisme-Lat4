package lat4;

/**
 *
 * @author nabila
 */
public class Triangle extends Shape {
    private int base;
    private int height;
    public Triangle(String color, int base, int height) {
        super(color);
        this.base = base;
        this.height = height;
    }
    public String toString() {
        return "Traingle [Base = " + base + ", Height = " + height + "," + super.toString() + "]";
    }
    public double getArea() {
        return 0.5*base*height;
    }
}
